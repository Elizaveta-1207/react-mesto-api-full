{
  "env": {
      "browser": true,
      "commonjs": true
      // "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "babel-eslint",
  "plugins": ["react", "react-hooks"],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules":{
    "no-console": 0,
    "no-underscore-dangle": ["error", {
       "allow": ["_id"]
    }],
    "quotes": ["error", "double"],
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error"
 }

}
